{
  "name": "server",
  "version": "0.1.0",
  "scripts": {
    "clean": "npx rimraf build/ node_modules/ .cache/ public/ **/__snapshots__/ coverage/ .nyc_output/",
    "build": "babel src --out-dir ./build",
    "start": "node ./build/app.js",
    "start:dev": "nodemon --exec babel-node ./src/app.js",
    "test": "mocha src/tests/ --recursive --exit --require @babel/register",
    "coverage": "nyc npm test"
  },
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/register": "^7.5.5",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.0",
    "nodemon": "^1.19.2",
    "pg-promise": "^8.7.5",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "codecov": "^3.3.0",
    "mocha": "^6.2.0",
    "nyc": "^14.0.0"
  },
  "proxy": "http://localhost:5000/",
  "nyc": {
    "require": [
      "babel/register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "all": true,
    "check-coverage": true,
    "sourceMap": false,
    "instrument": false
  }
}
